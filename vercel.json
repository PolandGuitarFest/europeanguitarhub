{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/subscribe",
      "dest": "/api/subscribe.js"
    },
    {
      "src": "/api/blog/(.*)",
      "dest": "/api/blog/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
